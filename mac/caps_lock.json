    {
      "description": "CapsLock to Hyper/Escape",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_held_down": [
            {
              "set_variable": {
                "name": "hyphern_mode",
                "value": 1
              }
            }
          ],
          "to": [
            {
              "key_code": "vk_none"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "hyphern_mode",
                "value": 0
              }
            }
          ]
        }
      ]
    },
